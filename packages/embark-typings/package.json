{
  "name": "embark-typings",
  "version": "4.0.0-beta.0",
  "author": "Iuri Matias <iuri.matias@gmail.com>",
  "contributors": [],
  "description": "Typescript types used by embark",
  "homepage": "https://embark.status.im/",
  "keywords": [
    "blockchain",
    "dapps",
    "ethereum",
    "ipfs",
    "serverless",
    "solc",
    "solidity"
  ],
  "license": "MIT",
  "repository": "github:embark-framework/embark",
  "scripts": {
    "ci": "npm run qa",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "package": "npm pack",
    "qa": "npm-run-all lint typecheck package",
    "reset": "npx rimraf node_modules",
    "start": "npm run watch",
    "typecheck": "tsc",
    "watch": "run-p watch:*",
    "watch:typecheck": "npm run typecheck -- --preserveWatchOutput --watch"
  },
  "devDependencies": {
    "eslint": "5.7.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "tslint": "5.11.0",
    "typescript": "3.1.3",
    "web3-eth-abi": "1.0.0-beta.37"
  },
  "engines": {
    "node": ">=8.12.0",
    "npm": ">=6.4.1",
    "yarn": ">=1.12.3"
  }
}
