    "qa": "npm-run-all _build",
    "reset": "npx rimraf coverage dist embarkjs-*.tgz package",
    "solo": "embark-solo"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.6.3",
    "core-js": "3.3.5",
    "ipfs-http-client": "40.0.0"
  },
  "devDependencies": {
    "ajv": "6.10.2",
    "embark-solo": "^5.0.0-alpha.0",
    "lodash.clonedeep": "4.5.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.0"
  },
  "engines": {
    "node": ">=10.17.0 <12.0.0",
    "npm": ">=6.11.3",
    "yarn": ">=1.19.1"
  }
